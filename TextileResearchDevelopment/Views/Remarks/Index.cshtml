            event.preventDefault();

            var Row = RemarksTable.rows('.selected').data()[0];
            var index = RemarksTable.row('.selected').index();

            var stenter =
            {
                Id: Row.Id,
                ApprovedTime: moment(Date.now()).format("YYYY/MM/DD h:mm a"),
            };

            $.ajax({
                type: "POST",
                url: "/Remarks/Approve",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                data: JSON.stringify(stenter),
                success: function (response) {
                    debugger
                    if (response.data != "" && response.data != null) {
