                        <div style="display: inline;">
                            <input type="number" class="form-control" style="width:60%;" id="WidthSet" min="0" step="1" data-bind="value:WidthSet" placeholder="Width Set" required autofocus>
                        </div>
                    </div>
                    <div class="form-group col-xs-4 form-inline">
                        <label for="OverFeed" class="control-label" style="width:35%;">OverFeed</label>
                        <div style="display: inline;">
                            <input type="number" class="form-control" style="width:60%;" id="OverFeed" min="0" step="1" data-bind="value:Serial" placeholder="OverFeed%" required autofocus>
