        $('#DeliveryDateSL, #FabricOrderDateSL, #KnitOrderDateSL').datetimepicker({
            defaultDate: new Date(),
            format: 'DD/MM/YYYY'
        });

        $('#modalSoftener').on('shown.bs.modal', function () {
            SoftenerTable.columns.adjust();
        });

        var StenterTable = $('#StenterTable').DataTable({
            "ajax":
            {
                "url": "/Stenter/GetData",
                "type": "GET",
                "dataType": "JSON"
            },
            "autowidth": false,
            "scrollY": true,
            "scrollX": true,
            "Sorting": true,
            "searching": true,
            "destroy": true,
            "select": {
                style: 'single'
            },
            "columnDefs": [
                {
                    "targets": [0],
                    "searchable": false,
                    "orderable": false,
                },
                {
                    "targets": [1, 2, 3],
                    "visible": false,
                    "searchable": false
                },
                {
                    "targets": "_all",
                    "width": "100px"
                }

            ],

            "columns": [
                {
                    "searchable": false,
                    "render": function (data, type, full, meta) {
                        return StenterTable.data().count();
                    }
                },
                { "data": "Id" },
                { "data": "DyeingID" },
                { "data": "SoftenerID" },
                { "data": "BarCode" },
